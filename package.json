    "type": "git",
    "url": "git+https://github.com/sbor767/dashboard-api-playground.git"
  },
  "bugs": {
    "url": "https://github.com/sbor767/dashboard-api-playground/issues",
    "email": "sbor767@gmail.com"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "express": "^4.22.1",
    "inversify": "^6.2.2",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.2.2",
    "tslog": "^3.3.4"
  },
