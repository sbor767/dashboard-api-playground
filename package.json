    "@types/jsonwebtoken": "^8.5.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "jest": "^27.5.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^5.22.0",
    "supertest": "^6.3.4",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
