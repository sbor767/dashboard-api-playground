  },
  "bugs": {
    "url": "https://github.com/sbor767/dashboard-api-playground/issues",
    "email": "sbor767@gmail.com"
  },
  "dependencies": {
    "@prisma/adapter-libsql": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "inversify": "^6.2.2",
